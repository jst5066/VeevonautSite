@model MvcVeevonauts.Models.Item

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<fieldset>
    <legend>Item</legend>

    <div class="display-label">ItemTypeID</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.ItemType.ItemTypeID)
    </div>

    <div class="display-label">Type</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Type.Name)
    </div>

    <div class="display-label">SubType</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.SubType)
    </div>

    <div class="display-label">Price</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Price)
    </div>

    <div class="display-label">ItemArtUrl</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.ItemArtUrl)
    </div>
</fieldset>
<p>
    @Html.ActionLink("Edit", "Edit", new { id=Model.ItemID }) |
    @Html.ActionLink("Back to List", "Index")
</p>
